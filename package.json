{
  "name": "regular",
  "private": true,
  "version": "1.0.0",
  "dependencies": {
    "gatsby": "2.18.2",
    "gatsby-plugin-feed": "2.3.21",
    "gatsby-plugin-html2amp": "1.3.1",
    "gatsby-plugin-netlify": "2.1.25",
    "gatsby-plugin-netlify-cms": "4.1.28",
    "gatsby-plugin-react-helmet": "3.1.15",
    "gatsby-plugin-react-svg": "2.1.2",
    "gatsby-plugin-sass": "2.1.23",
    "gatsby-plugin-sharp": "2.3.2",
    "gatsby-plugin-sitemap": "2.2.21",
    "gatsby-plugin-typography": "2.3.17",
    "gatsby-plugin-webpack-bundle-analyzer": "1.0.5",
    "gatsby-remark-autolink-headers": "2.1.18",
    "gatsby-remark-copy-linked-files": "2.1.30",
    "gatsby-remark-images": "3.1.33",
    "gatsby-remark-prismjs": "3.3.24",
    "gatsby-remark-responsive-iframe": "2.2.27",
    "gatsby-remark-smartypants": "2.1.16",
    "gatsby-source-filesystem": "2.1.38",
    "gatsby-transformer-remark": "2.6.37",
    "gatsby-transformer-sharp": "2.3.5",
    "github-markdown-css": "3.0.1",
    "immutable": "3.8.2",
    "netlify-cms": "2.9.7",
    "netlify-cms-lib-auth": "2.2.5",
    "node-sass": "4.13.0",
    "prismjs": "1.17.1",
    "prop-types": "15.7.2",
    "react": "16.10.2",
    "react-dom": "16.10.2",
    "react-helmet": "5.2.1",
    "react-typography": "0.16.19",
    "remark-breaks": "1.0.3",
    "reset-css": "5.0.1",
    "slate": "0.47.9",
    "typeface-merriweather": "0.0.72",
    "typeface-montserrat": "0.0.75",
    "typography": "0.16.19",
    "typography-theme-kirkham": "0.16.19"
  },
  "devDependencies": {
    "@babel/cli": "7.7.0",
    "@babel/core": "7.7.2",
    "amphtml-validator": "1.0.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.3",
    "babel-jest": "24.9.0",
    "babel-plugin-dynamic-import-node": "2.3.0",
    "babel-preset-gatsby": "0.2.22",
    "eslint": "6.7.0",
    "eslint-plugin-react": "7.16.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "24.9.0",
    "prettier": "1.19.1",
    "react-test-renderer": "16.10.2"
  },
  "scripts": {
    "dev": "npm run build:utils && gatsby develop",
    "lint": "eslint --ext .js,.jsx --ignore-pattern public .",
    "amp": "amphtml-validator public/post/**/*.html && amphtml-validator public/index.html",
    "unit": "jest",
    "test": "npm run lint && npm run unit && npm run amp",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write 'src/**/*.js' 'src/**/*.md'",
    "develop": "npm run build:utils && gatsby develop",
    "build": "npm run build:utils && gatsby build",
    "fix-semi": "eslint --quiet --ignore-pattern node_modules --ignore-pattern public --parser babel-eslint --no-eslintrc --rule '{\"semi\": [2, \"never\"], \"no-extra-semi\": [2]}' --fix gatsby-node.js",
    "build:utils": "babel src/utils/post.js --out-file src/utils/dist/post.js --config-file ./src/utils/.babelrc && babel src/utils/site.js --out-file src/utils/dist/site.js --config-file ./src/utils/.babelrc"
  }
}
