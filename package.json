{
  "name": "blog.tomoyukikashiro.me",
  "version": "1.0.0",
  "description": "my blog",
  "main": "",
  "devDependencies": {
    "amphtml-validator": "^1.0.20",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "node-sass": "^4.5.3",
    "psi": "^3.0.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^2.6.1",
    "webpack-manifest-plugin": "^1.1.0"
  },
  "config": {
    "outputdir": "theme/dist"
  },
  "scripts": {
    "test": "npm run test_amp && npm run test_psi",
    "build": "npm run clean && NODE_ENV=production webpack --config webpack.config.js",
    "clean": "if test -d $npm_package_config_outputdir; then rm -r $npm_package_config_outputdir; fi",
    "watch": "webpack --config webpack.config.js --watch",
    "test_amp": "amphtml-validator output/amp/post/**/*.html",
    "test_psi": "psi --threshold 70 --strategy mobile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomoyukikashiro/blog.tomoyukikashiro.me.git"
  },
  "author": "Tomoyuki Kashiro",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tomoyukikashiro/blog.tomoyukikashiro.me/issues"
  },
  "homepage": "https://github.com/tomoyukikashiro/blog.tomoyukikashiro.me#readme"
}
