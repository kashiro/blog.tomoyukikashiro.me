{
  "name": "regular",
  "private": true,
  "version": "1.0.0",
  "dependencies": {
    "gatsby": "2.8.2",
    "gatsby-plugin-feed": "2.2.2",
    "gatsby-plugin-google-tagmanager": "2.0.15",
    "gatsby-plugin-manifest": "2.1.1",
    "gatsby-plugin-netlify": "2.0.17",
    "gatsby-plugin-netlify-cms": "4.0.1",
    "gatsby-plugin-offline": "2.1.1",
    "gatsby-plugin-react-helmet": "3.0.12",
    "gatsby-plugin-react-svg": "2.1.1",
    "gatsby-plugin-sharp": "2.1.3",
    "gatsby-plugin-sitemap": "2.1.0",
    "gatsby-plugin-typography": "2.2.13",
    "gatsby-plugin-webpack-bundle-analyzer": "1.0.5",
    "gatsby-remark-copy-linked-files": "2.0.13",
    "gatsby-remark-images": "3.0.14",
    "gatsby-remark-prismjs": "3.2.9",
    "gatsby-remark-responsive-iframe": "2.1.1",
    "gatsby-remark-smartypants": "2.0.9",
    "gatsby-source-filesystem": "2.0.38",
    "gatsby-transformer-remark": "2.3.12",
    "gatsby-transformer-sharp": "2.1.21",
    "github-markdown-css": "3.0.1",
    "immutable": "3.8.2",
    "lodash": "4.17.11",
    "netlify-cms-lib-auth": "2.2.2",
    "netlify-cms": "2.9.1",
    "prismjs": "1.16.0",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-helmet": "5.2.1",
    "react-typography": "0.16.19",
    "reset-css": "4.0.1",
    "slate": "0.47.3",
    "typeface-merriweather": "0.0.72",
    "typeface-montserrat": "0.0.54",
    "typography": "0.16.19",
    "typography-theme-kirkham": "0.16.19"
  },
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.5",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "24.8.0",
    "babel-plugin-dynamic-import-node": "2.2.0",
    "babel-preset-gatsby": "0.1.11",
    "eslint": "5.16.0",
    "eslint-plugin-react": "7.14.2",
    "identity-obj-proxy": "3.0.0",
    "jest": "24.8.0",
    "prettier": "1.18.2",
    "react-test-renderer": "16.8.6"
  },
  "scripts": {
    "dev": "npm run build:utils && gatsby develop",
    "lint": "eslint --ext .js,.jsx --ignore-pattern public .",
    "unit": "jest",
    "test": "npm run lint && npm run unit",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write 'src/**/*.js' 'src/**/*.md'",
    "develop": "npm run build:utils && gatsby develop",
    "build": "npm run build:utils && gatsby build",
    "fix-semi": "eslint --quiet --ignore-pattern node_modules --ignore-pattern public --parser babel-eslint --no-eslintrc --rule '{\"semi\": [2, \"never\"], \"no-extra-semi\": [2]}' --fix gatsby-node.js",
    "build:utils": "babel src/utils/post.js --out-file src/utils/dist/post.js --config-file ./src/utils/.babelrc && babel src/utils/site.js --out-file src/utils/dist/site.js --config-file ./src/utils/.babelrc"
  }
}
