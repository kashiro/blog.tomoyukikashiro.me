{
  "name": "regular",
  "private": true,
  "version": "1.0.0",
  "dependencies": {
    "gatsby": "^2.1.17",
    "gatsby-plugin-feed": "^2.0.13",
    "gatsby-plugin-google-tagmanager": "^2.0.9",
    "gatsby-plugin-manifest": "^2.0.10",
    "gatsby-plugin-netlify": "^2.0.5",
    "gatsby-plugin-netlify-cms": "^3.0.7",
    "gatsby-plugin-offline": "^2.0.17",
    "gatsby-plugin-react-helmet": "^3.0.2",
    "gatsby-plugin-react-svg": "^2.0.0",
    "gatsby-plugin-sharp": "^2.0.22",
    "gatsby-plugin-sitemap": "^2.0.2",
    "gatsby-plugin-typography": "^2.2.1",
    "gatsby-plugin-webpack-bundle-analyzer": "^1.0.3",
    "gatsby-remark-copy-linked-files": "^2.0.6",
    "gatsby-remark-images": "^3.0.0",
    "gatsby-remark-prismjs": "^3.0.3",
    "gatsby-remark-responsive-iframe": "^2.0.6",
    "gatsby-remark-smartypants": "^2.0.6",
    "gatsby-source-filesystem": "^2.0.22",
    "gatsby-transformer-remark": "^2.1.12",
    "gatsby-transformer-sharp": "^2.1.8",
    "github-markdown-css": "^3.0.1",
    "immutable": "^3.8.2",
    "lodash": "^4.17.11",
    "netlify-cms": "^2.2.0",
    "netlify-cms-lib-auth": "^2.0.5",
    "prismjs": "^1.15.0",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-helmet": "^5.2.0",
    "react-typography": "^0.16.13",
    "reset-css": "^4.0.1",
    "slate": "^0.44.6",
    "typeface-merriweather": "0.0.72",
    "typeface-montserrat": "0.0.54",
    "typography": "^0.16.17",
    "typography-theme-kirkham": "^0.16.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "24.3.1",
    "babel-plugin-dynamic-import-node": "2.2.0",
    "babel-preset-gatsby": "0.1.8",
    "eslint": "5.15.1",
    "eslint-plugin-react": "7.12.4",
    "identity-obj-proxy": "3.0.0",
    "jest": "24.3.1",
    "prettier": "1.16.4",
    "react-test-renderer": "16.8.4"
  },
  "scripts": {
    "dev": "npm run build:utils && gatsby develop",
    "lint": "eslint --ext .js,.jsx --ignore-pattern public .",
    "unit": "jest",
    "test": "npm run lint && npm run unit",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write 'src/**/*.js' 'src/**/*.md'",
    "develop": "npm run build:utils && gatsby develop",
    "build": "npm run build:utils && gatsby build",
    "fix-semi": "eslint --quiet --ignore-pattern node_modules --ignore-pattern public --parser babel-eslint --no-eslintrc --rule '{\"semi\": [2, \"never\"], \"no-extra-semi\": [2]}' --fix gatsby-node.js",
    "build:utils": "babel src/utils/post.js --out-file src/utils/dist/post.js --config-file ./src/utils/.babelrc && babel src/utils/site.js --out-file src/utils/dist/site.js --config-file ./src/utils/.babelrc"
  }
}
