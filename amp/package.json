{
  "name": "amp",
  "private": true,
  "version": "1.0.0",
  "dependencies": {
    "gatsby": "2.3.4",
    "gatsby-plugin-html2amp": "1.0.0",
    "gatsby-plugin-netlify": "2.0.13",
    "gatsby-plugin-react-helmet": "3.0.11",
    "gatsby-plugin-react-svg": "2.1.1",
    "gatsby-plugin-sharp": "2.0.32",
    "gatsby-plugin-sitemap": "2.0.11",
    "gatsby-plugin-typography": "2.2.10",
    "gatsby-remark-copy-linked-files": "2.0.11",
    "gatsby-remark-images": "3.0.10",
    "gatsby-remark-prismjs": "3.2.6",
    "gatsby-remark-responsive-iframe": "2.1.1",
    "gatsby-remark-smartypants": "2.0.9",
    "gatsby-source-filesystem": "2.0.28",
    "gatsby-transformer-remark": "2.3.8",
    "gatsby-transformer-sharp": "2.1.17",
    "github-markdown-css": "3.0.1",
    "glob": "7.1.3",
    "immutable": "3.8.2",
    "lodash": "4.17.11",
    "prismjs": "1.16.0",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-helmet": "5.2.1",
    "react-typography": "0.16.19",
    "reset-css": "4.0.1",
    "slate": "0.44.12",
    "typeface-merriweather": "0.0.72",
    "typeface-montserrat": "0.0.54",
    "typography": "0.16.19",
    "typography-theme-kirkham": "0.16.19"
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.4.0",
    "amphtml-validator": "1.0.23",
    "eslint": "5.15.3",
    "eslint-plugin-react": "7.12.4",
    "prettier": "1.17.0"
  },
  "scripts": {
    "dev": "npm run build:utils && gatsby develop",
    "lint": "eslint --ext .js,.jsx --ignore-pattern public .",
    "amp": "amphtml-validator public/post/**/index.html",
    "test": "npm run lint && npm run amp",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write 'src/**/*.js' 'src/**/*.md'",
    "develop": "npm run build:utils && gatsby develop",
    "build": "npm run build:utils && gatsby build",
    "build:utils": "babel ../regular/src/utils/post.js --out-file ../regular/src/utils/dist/post.js --config-file ../regular/src/utils/.babelrc && babel ../regular/src/utils/site.js --out-file ../regular/src/utils/dist/site.js --config-file ../regular/src/utils/.babelrc"
  }
}
